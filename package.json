{
  "name": "memu-bot",
  "version": "1.0.0",
  "description": "memU bot - AI Assistant for messaging platforms",
  "main": "./out/main/index.js",
  "engines": {
    "node": "24.13.0"
  },
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "start": "electron-vite preview",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "tsx scripts/build-win.ts",
    "build:mac": "tsx scripts/build-mac.ts",
    "build:linux": "npm run build && electron-builder --linux",
    "release:mac": "tsx scripts/release-mac.ts",
    "release:win": "tsx scripts/release-win.ts",
    "dev:memu": "APP_MODE=memu electron-vite dev --mode memu",
    "dev:yumi": "APP_MODE=yumi electron-vite dev --mode yumi",
    "build:memu": "APP_MODE=memu electron-vite build --mode memu",
    "build:yumi": "APP_MODE=yumi electron-vite build --mode yumi",
    "build:memu:mac": "APP_MODE=memu tsx scripts/build-mac.ts",
    "build:memu:win": "tsx scripts/build-win.ts --mode memu",
    "build:yumi:mac": "APP_MODE=yumi tsx scripts/build-mac.ts",
    "build:yumi:win": "tsx scripts/build-win.ts --mode yumi",
    "prepare-mode": "tsx scripts/prepare-mode.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-cloudfront": "^3.978.0",
